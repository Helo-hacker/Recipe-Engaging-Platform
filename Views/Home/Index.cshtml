@{
    ViewBag.Title = "Home";
}

<style>
/* --- HERO SECTION THEME --- */
.hero-wrapper {
    position: relative;
    background: linear-gradient(135deg, #fff7f1, #fef1e7);
    border-radius: 24px;
    padding: 70px 55px;
    overflow: hidden;
    box-shadow: 0 8px 25px rgba(0,0,0,0.06);
}

/* FOOD BACKGROUND ART */
.hero-bg {
    position: absolute;
    top: -40px;
    right: -20px;
    width: 420px;
    opacity: 0.39;
    filter: blur(1px);
    pointer-events: none;
}

.hero-title {
    font-size: 42px;
    font-weight: 800;
    color: #4b2e24;
    letter-spacing: -0.5px;
}

.hero-sub {
    font-size: 18px;
    color: #7a5c52;
    max-width: 520px;
    margin-top: 12px;
}

/* CTA BUTTON */
.hero-btn {
    background: #e67a57;
    border: none;
    padding: 12px 28px;
    color: white;
    font-size: 18px;
    border-radius: 14px;
    font-weight: 600;
    margin-top: 25px;
    transition: 0.25s ease;
        text-decoration: none !important;

}

.hero-btn:hover {
    background: #d8643f;
    transform: translateY(-2px);
    box-shadow: 0 8px 20px rgba(230, 122, 87, 0.4);
}

/* FLOATING FOOD DECORATIONS */
.hero-float {
    position: absolute;
    width: 100px;
    opacity: 0.85;
    animation: floatFood 6s ease-in-out infinite;
}

.hero-float.f1 { top: 20px; left: -30px; animation-delay: 0s; }
.hero-float.f2 { bottom: 10px; right: 20px; width: 80px; animation-delay: 1.5s; }

@@keyframes floatFood {
    0% { transform: translateY(0px); }
    50% { transform: translateY(-12px); }
    100% { transform: translateY(0px); }
}
</style>


<div class="hero-wrapper">

    @* <!-- Subtle decorative floating images -->
    <img class="hero-float f1" src="https://images.unsplash.com/photo-1565299585323-38d6b0865b47?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MzV8fGZvb2R8ZW58MHx8MHx8fDA%3D" />
    <img class="hero-float f2" src="https://plus.unsplash.com/premium_photo-1663858367001-89e5c92d1e0e?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTN8fGZvb2R8ZW58MHx8MHx8fDA%3D" /> *@

    <!-- Background food blur art -->
    <img class="hero-bg"
         src="https://images.unsplash.com/photo-1549590143-d5855148a9d5?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTR8fGNvb2tpbmd8ZW58MHx8MHx8fDA%3D" />

    <!-- Main Heading -->
    <h1 class="hero-title">Discover & Share Amazing Recipes</h1>

    <p class="hero-sub">
        Explore delicious dishes, follow creators, save your favorites, and share
        your culinary creativity with the world.
    </p>

    @if (Context.Session.GetInt32("UserId") == null)
    {
        <a class="hero-btn" href="/Account/Register">Get Started</a>
    }
    else
    {
        <a class="hero-btn" href="/Dashboard">Go to Dashboard</a>
    }

</div>

<!-- POWERFUL FOOD SLOGAN SECTION -->
<div class="food-slogan-section my-5 text-center">
    <h2 class="fw-bold" style="font-size: 34px;">
        “Great meals are meant to be shared.”
    </h2>
    <p class="mt-2 text-muted" style="font-size: 18px;">
        Discover recipes that warm the heart, fuel your passion, and bring people together.
    </p>
</div>


<!-- CINEMATIC FOOD IMAGE BANNER -->
<style>
.food-banner {
    width: 100%;
    height: 360px;
    border-radius: 20px;
    overflow: hidden;
    position: relative;
    box-shadow: 0 12px 30px rgba(0,0,0,0.15);
    margin-bottom: 40px;
}

.food-banner img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: 0.5s ease;
}

.food-banner:hover img {
    transform: scale(1.1);
    filter: brightness(1.15);
}

.food-banner-text {
    position: absolute;
    bottom: 30px;
    left: 40px;
    color: white;
    text-shadow: 0 4px 12px rgba(0,0,0,0.6);
}

.food-banner-text h2 {
    font-size: 36px;
    font-weight: 800;
}

.food-banner-text p {
    font-size: 18px;
    opacity: 0.9;
}
</style>

<div class="food-banner">
    <img src="https://images.unsplash.com/photo-1504674900247-0877df9cc836?auto=format&fit=crop&w=1200&q=80" alt="Food Banner">
    <div class="food-banner-text">
        <h2>Culinary inspiration at your fingertips</h2>
        <p>Explore trending dishes loved across the world</p>
    </div>
</div>



<!-- PROMOTION / AD CARD -->
<style>
.promo-card {
    background: linear-gradient(135deg, #ffe8d9, #ffd5c2);
    border-radius: 18px;
    padding: 28px;
    display: flex;
    align-items: center;
    gap: 25px;
    margin-bottom: 50px;
    box-shadow: 0 10px 25px rgba(0,0,0,0.15);
    cursor: pointer;
    transition: 0.3s ease;
}

.promo-card:hover {
    transform: translateY(-4px);
}

.promo-card img {
    width: 160px;
    height: 140px;
    border-radius: 16px;
    object-fit: cover;
    box-shadow: 0 6px 16px rgba(0,0,0,0.25);
}

.promo-title {
    font-size: 26px;
    font-weight: 700;
    color: #6b412d;
}

.promo-text {
    font-size: 16px;
    color: #7a5543;
}
</style>

<div class="promo-card" onclick="window.location='/Recipe/List'">
    <img src="data:image/jpeg;base64," alt="Promo image" />
    <div>
        <div class="promo-title">Unlock Your Next Favorite Dish</div>
        <div class="promo-text">
            Explore hand-picked collections created just for food lovers like you.
        </div>
    </div>
</div>



<h3 class="mt-5 mb-3">Popular Categories</h3>
<div class="row g-4">
    <div class="col-md-3">
        <div class="card shadow-sm">
            <div class="card-body">
                <h5>Breakfast</h5>
                <p>Start your day with energy</p>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card shadow-sm">
            <div class="card-body">
                <h5>Lunch & Dinner</h5>
                <p>Hearty meals for the family</p>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card shadow-sm">
            <div class="card-body">
                <h5>Snacks</h5>
                <p>Quick bites and fun food</p>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card shadow-sm">
            <div class="card-body">
                <h5>Tea</h5>
                <p>Refresh your mind & mood</p>
            </div>
        </div>
    </div>
</div>
